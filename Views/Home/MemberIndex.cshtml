@model IEnumerable<ApplicationUser>

<div class="bg-primary panel-body">
    <h4>Member Accounts</h4>
</div>
<hr/>
<div class="container">
    <table class="table table-condensed table-striped table-bordered">
        <tr>
            <th class="text-center">Username</th>
            <th class="text-center">First Name</th>
            <th class="text-center">Last Name</th>
            <th class="text-center">Email Address</th>
            <th class="text-center">Modeling Interest</th>
            <th class="text-center">Manage Account</th>
        </tr>
        @foreach(ApplicationUser user in Model){
            <tr>
                <td><a href="/members/@user.UserName">@user.UserName</a></td>
                <td>@user.FirstName</td>
                <td>@user.LastName</td>
                <td>@user.Email</td>
                <td>@user.ModelingInterest</td>
                <td>
                    <form action="/members/@user.UserName/delete" method="POST">
                        @*<a href="/members/@user.UserName/edit" class="btn btn-sm btn-primary">Edit User</a>*@
                        @*<a href="members/@user.UserName/delete" class="btn btn-sm btn-danger">Delete User</a>*@

                        @* <a type="button" class="btn btn-sm btn-primary" href="/members/@Model.UserName/stash/@item.ItemID/edit" method="get">Edit Item</a>*@
                        @* <input type="hidden" name="ItemID" value="@item.ItemID"/> *@
                        <button type="submit" class="btn btn-sm btn-danger">Delete User</button>
                        @* <a type="button" class="btn btn-sm btn-danger" href="/members/@Model.UserName/stash/@item.ItemID/delete" method="post">Delete Item</a> *@
                    </form>


                    @*<form asp-controller="Account" asp-action="Delete" asp-method="post" asp-route-id="@user.UserName">
                        <a class="btn btn-sm btn-primary" asp-action="Edit" asp-route-id="@user.UserName">Edit</a>
                        <button type="submit" class="btn btn-sm btn-danger">Delete</button>
                    </form>*@
                </td>                   
            </tr>
        }
    </table>
</div>