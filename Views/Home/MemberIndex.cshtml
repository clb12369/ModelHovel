@model IEnumerable<ApplicationUser>

<div class="bg-primary panel-body">
    <h4>Member Accounts</h4>
</div>

<table class="table table-condensed table-striped table-bordered">
    <tr>
        <th>Username</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email Address</th>
    </tr>
    @foreach(ApplicationUser user in Model){
        <tr>
            <td><a href="/members/@user.UserName">@user.UserName</a></td>
            <td>@user.FirstName</td>
            <td>@user.LastName</td>
            <td>@user.Email</td>
        </tr>
    }
</table>